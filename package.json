{
  "name": "send-cli",
  "version": "0.2.0",
  "type": "module",
  "description": "Send is a CLI tool to instantly share files, folders, and links with mobile devices using QR codes - without the need for an external app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "ts": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "file sharing",
    "qr code",
    "mobile",
    "command line",
    "file transfer",
    "directory sharing",
    "cross platform",
    "instant sharing",
    "no app required",
    "file server",
    "mobile transfer",
    "share files",
    "local sharing",
    "wireless transfer"
  ],
  "bin": {
    "send": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "koushik yemula",
  "license": "MIT",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koushikyemula/send-cli.git"
  },
  "bugs": {
    "url": "https://github.com/koushikyemula/send-cli/issues"
  },
  "homepage": "https://github.com/koushikyemula/send-cli#readme",
  "dependencies": {
    "@types/express-fileupload": "^1.5.1",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/serve-handler": "^6.1.4",
    "chalk": "^4.1.2",
    "clipboardy": "^3.0.0",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-fileupload": "^1.5.1",
    "multer": "^1.4.5-lts.1",
    "ora": "^5.4.1",
    "portfinder": "^1.0.32",
    "qrcode-terminal": "^0.12.0",
    "serve-handler": "^6.1.6",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
